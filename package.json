{
  "name": "electron-app",
  "version": "1.0.0",
  "description": "Sofweb: TypeScript + Angular 2 + Webpack + Electron + SASS SCSS + HTML5 + Bootstrap",
  "main": "src/dist/index.js",
  "scripts": {
    "webpack": "webpack --config config/webpack/webpack.config.js --progress --profile --colors --display-error-details --display-cached",
    "webpack-watch": "npm run webpack --watch",
    "webpack-server": "cross-env PORT=8080 cross-env HOST=localhost webpack-dev-server --config config/webpack/webpack.config.js --inline",
    "production": "cross-env NODE_ENV=production npm run webpack",
    "test": "cross-env NODE_ENV=test npm run webpack",
    "electron": "electron .",
    "postinstall": "typings install",
    "typings": "typings"
  },
  "author": "Israel Gilyadov",
  "license": "MIT",
  "dependencies": {
    "@angular/common": "2.0.0-rc.4",
    "@angular/compiler": "2.0.0-rc.4",
    "@angular/core": "2.0.0-rc.4",
    "@angular/forms": "0.2.0",
    "@angular/http": "2.0.0-rc.4",
    "@angular/platform-browser": "2.0.0-rc.4",
    "@angular/platform-browser-dynamic": "2.0.0-rc.4",
    "@angular/router": "3.0.0-beta.2",
    "@angular/router-deprecated": "2.0.0-rc.2",
    "@angular/upgrade": "2.0.0-rc.4",
    "es6-shim": "0.35.1",
    "core-js": "2.4.1",
    "reflect-metadata": "0.1.3",
    "rxjs": "5.0.0-beta.6",
    "zone.js": "0.6.12",
    "angular2-in-memory-web-api": "0.0.14",
    "bootstrap": "3.3.7"
  },
  "devDependencies": {
    "angular2-template-loader": "0.4.0",
    "clean-webpack-plugin": "^0.1.10",
    "copy-webpack-plugin": "^3.0.1",
    "cross-env": "2.0.0",
    "css-loader": "0.23.1",
    "electron-prebuilt": "1.3.0",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.9.0",
    "html-loader": "0.4.3",
    "html-webpack-plugin": "2.22.0",
    "json-loader": "0.5.4",
    "node-sass": "^3.8.0",
    "sass-loader": "^4.0.0",
    "style-loader": "0.13.1",
    "to-string-loader": "1.1.4",
    "ts-loader": "0.8.2",
    "typescript": "1.8.10",
    "typings": "1.3.2",
    "webpack": "1.13.1",
    "webpack-dev-server": "1.14.1",
    "webpack-merge": "0.14.1",
    "webpack-target-electron-renderer": "0.4.0"
  }
}
